<%= render partial: 'message' %>

<fieldset>
<legend>User Login</legend>
<%= form_for(@user, url: sessions_path) do |f| %>
    <%= render 'users/credentials', {f: f, user: @user} %>

    <%= f.submit 'Log in'%>
<% end %>
</fieldset>

<p>
<%= link_to('Log in with GitHub!', '/auth/github') %>
</p>

<p>
<%= link_to('Sign up', signup_path) %>
</p>
